{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "es2020",
    "module": "es2020",
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
